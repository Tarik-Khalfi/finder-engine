\n\r\n    if (type.includes(\"/news\")) {\r\n      setResults(data.entries);\r\n    } else if (type.includes(\"/images\")) {\r\n      setResults(data.image_results);\r\n    } else {\r\n      setResults(data.results);\r\n    }\r\n\r\n    setIsLoading(false);\r\n  };\r\n  return (\r\n    <ResultContext.Provider\r\n      value={{ getResults, results, searchTerm, setSearchTerm, isLoading }}\r\n    >\r\n      {children}\r\n    </ResultContext.Provider>\r\n  );\r\n};\r\nexport const useResultContext = () => useContext(ResultContext);\r\n","import React from \"react\";\r\nimport { NavLink } from \"react-router-dom\";\r\n\r\nconst links = [\r\n  { url: \"/search\", text: \"🔎All\" },\r\n  { url: \"/news\", text: \"📰 news\" },\r\n  { url: \"/images\", text: \"📷images\" },\r\n  { url: \"/videos\", text: \"📼videos\" },\r\n];\r\n\r\nexport const Links = () => {\r\n  return (\r\n    <div className=\"flex sm:justify-around justify-between items-center mt-4\">\r\n      {links.map(({ url, text }) => (\r\n        <NavLink\r\n          to={url}\r\n          className=\"m-2 mb-0\"\r\n          activeClassName=\"text-blue-700 border-b-2 dark:text-blue-300 border-blue-700 pb-2\"\r\n        >\r\n          {text}\r\n        </NavLink>\r\n      ))}\r\n    </div>\r\n  );\r\n};\r\n","import React, { useEffect, useState } from \"react\";\r\nimport { useDebounce } from \"use-debounce\";\r\nimport { useResultContext } from \"../contexts/ResultContextProvider\";\r\nimport { Links } from \"./Links\";\r\nexport const Search = () => {\r\n  const [text, setText] = useState(\"\");\r\n  const { setSearchTerm } = useResultContext();\r\n  const [debouncedValue] = useDebounce(text, 300);\r\n  useEffect(() => {\r\n    if (debouncedValue) setSearchTerm(debouncedValue);\r\n  }, [debouncedValue]);\r\n  return (\r\n    <div className=\"relative sm:ml-48 md:ml-72 sm:-mt-10 mt-3\">\r\n      <input\r\n        value={text}\r\n        type=\"text\"\r\n        className=\"sm:w-96 w-80 h-10 dark:bg-gray-200 border rounded-full shadow-sm outline-none p-6 text-black hover:shadow-lg\"\r\n        placeholder=\"Search Findder or Type URL\"\r\n        onChange={(e) => setText(e.target.value)}\r\n      />\r\n      {!text && (\r\n        <button\r\n          type=\"button\"\r\n          className=\"absolute top-1.5 right-4 text-2xl text-gray-500\"\r\n          onCLick={() => setText(\"\")}\r\n        >\r\n          X\r\n        </button>\r\n      )}\r\n      <Links />\r\n    </div>\r\n  );\r\n};\r\n","import React from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { Search } from \"./Search\";\r\nexport const Navbar = ({ darkTheme, setDarkTheme }) => {\r\n  return (\r\n    <div className=\"p-5 pb-0 flex flex-wrap sm:justify-betweem justify-center items-center border-b dark:border-gray-700 border-gray-200\">\r\n      <div className=\"flex justify-between items-center space-x-5 w-screen\">\r\n        <Link to=\"./\">\r\n          <p className=\"text-2xl bg-blue-500 font-bold text-white py-1 px-2 rounded  dark:bg-gray-500 dark:text-gray-900\">\r\n            Finder🎲\r\n          </p>\r\n        </Link>\r\n        <button\r\n          type=\"button\"\r\n          onClick={() => setDarkTheme(!darkTheme)}\r\n          className=\"text-xl dark:bg-gray-50 dark:text-gray-900 bg-white rounded-full px-2 py-1 hover:shadow-lg\"\r\n        >\r\n          {darkTheme ? \"Light💡\" : \"Dark🌙\"}\r\n        </button>\r\n      </div>\r\n      <Search />\r\n    </div>\r\n  );\r\n};\r\n","import React from \"react\";\r\n\r\nexport const Footer = () => {\r\n  return (\r\n    <div>\r\n      <div className=\"text-center p-10 mt-10 border-t dark:border-gray-700 border-gray-200\">\r\n        <h1>2021 Finder, Inc.</h1>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n","import React from \"react\";\r\nimport Loader from \"react-loader-spinner\";\r\nexport const Loading = () => {\r\n  return (\r\n    <div className=\"flex justify-center items-center\">\r\n      <Loader type=\"Puff\" color=\"#00BFFF\" height={550} width={80} />\r\n    </div>\r\n  );\r\n};\r\n","import React, { useEffect } from \"react\";\r\nimport { useLocation } from \"react-router-dom\";\r\nimport ReactPlayer from \"react-player\";\r\nimport { Loading } from \"./Loading\";\r\nimport { useResultContext } from \"../contexts/ResultContextProvider\";\r\nexport const Results = () => {\r\n  const { results, isLoading, searchTerm, getResults } = useResultContext();\r\n  const location = useLocation();\r\n  useEffect(() => {\r\n    if (searchTerm) {\r\n      if (location.pathname === \"/videos\") {\r\n        getResults(`/search/q=${searchTerm} videos`);\r\n      } else {\r\n        getResults(`${location.pathname}/q=${searchTerm}&num=100`);\r\n      }\r\n    }\r\n  }, [searchTerm, location.pathname]);\r\n  if (isLoading) return <Loading />;\r\n\r\n  switch (location.pathname) {\r\n    case \"/search\":\r\n      return (\r\n        <div className=\"flex flex-wrap justify-between space-y-6 sm:px-56 justify-between\">\r\n          {results?.map(({ link, title, description }, index) => (\r\n            <div key={index} className=\"md:w-2/5 w-full\">\r\n              <a href={link} target=\"_blank\" rel=\"noreferrer\">\r\n                <p className=\"text-sm\">\r\n                  {link.length > 30 ? link.substring(0, 30) : link}\r\n                </p>\r\n                <p className=\"text-lg hover:underline dark:text-blue-300 text-blue-700\">\r\n                  {title}\r\n                </p>\r\n                <p className=\"text-sm\">{description}</p>\r\n              </a>\r\n            </div>\r\n          ))}\r\n        </div>\r\n      );\r\n    case \"/images\":\r\n      return (\r\n        <div className=\"flex flex-wrap justify-center items-center\">\r\n          {results?.map(({ image, link: { href, title } }, index) => (\r\n            <a\r\n              className=\"sm:p-3 p-5\"\r\n              href={href}\r\n              key={index}\r\n              target=\"_blank\"\r\n              rel=\"noreferrer\"\r\n            >\r\n              <img src={image?.src} alt={title} loading=\"lazy\" />\r\n              <p className=\"w-36 break-words text-sm mt-2\">{title}</p>\r\n            </a>\r\n          ))}\r\n        </div>\r\n      );\r\n    case \"/news\":\r\n      return (\r\n        <div className=\"flex flex-wrap justify-between space-y-6 sm:px-56 items-center justify-between\">\r\n          {results?.map(({ links, id, source, title }) => (\r\n            <div key={id} className=\"md:w-2/5 w-full\">\r\n              <a\r\n                href={links?.[0].href}\r\n                target=\"_blank\"\r\n                rel=\"noreferrer\"\r\n                className=\"hover:underline\"\r\n              >\r\n                <p className=\"text-lg  dark:text-blue-300 text-blue-700\">\r\n                  {title}\r\n                </p>\r\n              </a>\r\n              <div className=\"flex gap-4\">\r\n                <a href={source?.href} target=\"_blank\" rel=\"noreferrer\">\r\n                  {source?.href}\r\n                </a>\r\n              </div>\r\n            </div>\r\n          ))}\r\n        </div>\r\n      );\r\n    case \"/videos\":\r\n      return (\r\n        <div className=\"flex flex-wrap ml-auto mr-auto items-center sm:items-center ml-auto mr-auto\">\r\n          {results?.map((video, index) => (\r\n            <div key={index} className=\"p-2\">\r\n              {video?.additional_links?.[0]?.href && (\r\n                <ReactPlayer\r\n                  url={video.additional_links?.[0].href}\r\n                  controls\r\n                  width=\"355px\"\r\n                  height=\"200px\"\r\n                />\r\n              )}\r\n            </div>\r\n          ))}\r\n        </div>\r\n      );\r\n\r\n    default:\r\n      return \"ERROR!\";\r\n  }\r\n};\r\n","import React from \"react\";\r\nimport { Switch, Route, Redirect } from \"react-router-dom\";\r\nimport { Results } from \"./Results\";\r\nexport const Routes = () => {\r\n  return (\r\n    <div>\r\n      <Switch>\r\n        <Route exact path=\"/\">\r\n          <Redirect to=\"/search\" />\r\n        </Route>\r\n        <Route exact path={[\"/search\", \"/images\", \"/news\", \"/videos\"]}>\r\n          <Results />\r\n        </Route>\r\n      </Switch>\r\n    </div>\r\n  );\r\n};\r\n","import React, { useState } from \"react\";\r\nimport { Navbar } from \"./components/Navbar\";\r\nimport { Footer } from \"./components/Footer\";\r\nimport { Routes } from \"./components/Routes\";\r\nconst App = () => {\r\n  const [darkTheme, setDarkTheme] = useState(false);\r\n  return (\r\n    <div className={darkTheme ? \"dark\" : \"\"}>\r\n      <div className=\"bg-gray-100 dark:bg-gray-900 dark:text-gray-200 min-h-screen\">\r\n        <Navbar darkTheme={darkTheme} setDarkTheme={setDarkTheme} />\r\n        <Routes />\r\n        <Footer />\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default App;\r\n","import React from \"react\";\r\nimport ReactDOM from \"react-dom\";\r\nimport App from \"./App\";\r\nimport \"./global.css\";\r\nimport { ResultContextProvider } from \"./contexts/ResultContextProvider\";\r\nimport { BrowserRouter as Router } from \"react-router-dom\";\r\nconst root = document.getElementById(\"root\");\r\n\r\nReactDOM.render(\r\n  <ResultContextProvider>\r\n    <Router>\r\n      <App />\r\n    </Router>\r\n  </ResultContextProvider>,\r\n  root\r\n);\r\n"]}}